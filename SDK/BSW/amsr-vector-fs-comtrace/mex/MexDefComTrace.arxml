<?xml version="1.0" encoding="utf-8" standalone="no"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://autosar.org/schema/r4.0 AUTOSAR_00049.xsd">
  <AR-PACKAGES>
    <AR-PACKAGE>
      <SHORT-NAME>MICROSAR</SHORT-NAME>
      <AR-PACKAGES>
        <AR-PACKAGE>
          <SHORT-NAME>ADAPTIVE</SHORT-NAME>
          <ELEMENTS>
            <SDG-DEF>
              <SHORT-NAME>ComTraceModelExtension</SHORT-NAME>
              <ADMIN-DATA>
                <SDGS>
                  <SDG GID="DvMex-version">
                    <SD>1.0.0</SD>
                  </SDG>
                </SDGS>
              </ADMIN-DATA>
              <SDG-CLASSES>
                <SDG-CLASS>
                  <SHORT-NAME>ComTraceClientServerOperationModelExtension</SHORT-NAME>
                  <DESC>
                    <L-2 L="EN">ComTrace specific model extension for ClientServerOperation (Method).</L-2>
                  </DESC>
                  <EXTENDS-META-CLASS>ClientServerOperation</EXTENDS-META-CLASS>
                  <ATTRIBUTES>
                    <SDG-AGGREGATION-WITH-VARIATION>
                      <SHORT-NAME>traceConfig</SHORT-NAME>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <VARIATION>false</VARIATION>
                      <SUB-SDG-REF DEST="SDG-CLASS">/MICROSAR/ADAPTIVE/ComTraceModelExtension/TraceMethodConfig</SUB-SDG-REF>
                    </SDG-AGGREGATION-WITH-VARIATION>
                  </ATTRIBUTES>
                </SDG-CLASS>
                <SDG-CLASS>
                  <SHORT-NAME>ComTraceVariableDataPrototypeModelExtension</SHORT-NAME>
                  <DESC>
                    <L-2 L="EN">ComTrace specific model extension for VariableDataPrototype (Event).</L-2>
                  </DESC>
                  <EXTENDS-META-CLASS>VariableDataPrototype</EXTENDS-META-CLASS>
                  <ATTRIBUTES>
                    <SDG-AGGREGATION-WITH-VARIATION>
                      <SHORT-NAME>traceConfig</SHORT-NAME>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <VARIATION>false</VARIATION>
                      <SUB-SDG-REF DEST="SDG-CLASS">/MICROSAR/ADAPTIVE/ComTraceModelExtension/TraceEventConfig</SUB-SDG-REF>
                    </SDG-AGGREGATION-WITH-VARIATION>
                  </ATTRIBUTES>
                </SDG-CLASS>
                <SDG-CLASS>
                  <SHORT-NAME>ComTraceFieldModelExtension</SHORT-NAME>
                  <DESC>
                    <L-2 L="EN">ComTrace specific model extension for Field.</L-2>
                  </DESC>
                  <EXTENDS-META-CLASS>Field</EXTENDS-META-CLASS>
                  <ATTRIBUTES>
                    <SDG-AGGREGATION-WITH-VARIATION>
                      <SHORT-NAME>traceConfigGetter</SHORT-NAME>
                      <DESC>
                        <L-2 L="EN">Trace configuration for the field getter method.</L-2>
                      </DESC>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <VARIATION>false</VARIATION>
                      <SUB-SDG-REF DEST="SDG-CLASS">/MICROSAR/ADAPTIVE/ComTraceModelExtension/TraceMethodConfig</SUB-SDG-REF>
                    </SDG-AGGREGATION-WITH-VARIATION>
                    <SDG-AGGREGATION-WITH-VARIATION>
                      <SHORT-NAME>traceConfigSetter</SHORT-NAME>
                      <DESC>
                        <L-2 L="EN">Trace configuration for the field setter method.</L-2>
                      </DESC>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <VARIATION>false</VARIATION>
                      <SUB-SDG-REF DEST="SDG-CLASS">/MICROSAR/ADAPTIVE/ComTraceModelExtension/TraceMethodConfig</SUB-SDG-REF>
                    </SDG-AGGREGATION-WITH-VARIATION>
                    <SDG-AGGREGATION-WITH-VARIATION>
                      <SHORT-NAME>traceConfigNotifier</SHORT-NAME>
                      <DESC>
                        <L-2 L="EN">Trace configuration for the field notifier event.</L-2>
                      </DESC>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <VARIATION>false</VARIATION>
                      <SUB-SDG-REF DEST="SDG-CLASS">/MICROSAR/ADAPTIVE/ComTraceModelExtension/TraceFieldNotifierConfig</SUB-SDG-REF>
                    </SDG-AGGREGATION-WITH-VARIATION>
                  </ATTRIBUTES>
                </SDG-CLASS>
                <SDG-CLASS>
                  <SHORT-NAME>TraceMethodConfig</SHORT-NAME>
                  <ATTRIBUTES>
                    <SDG-PRIMITIVE-ATTRIBUTE>
                      <SHORT-NAME>traceRequestSend</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">Enable / Disable tracing of transmitted ara::com method requests. Default: disabled.</L-2>
                      </DESC>
                      <CATEGORY>BOOLEAN</CATEGORY>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                    </SDG-PRIMITIVE-ATTRIBUTE>
                    <SDG-PRIMITIVE-ATTRIBUTE>
                      <SHORT-NAME>traceRequestReceived</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">Enable / Disable tracing of received ara::com method requests. Default: disabled.</L-2>
                      </DESC>
                      <CATEGORY>BOOLEAN</CATEGORY>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                    </SDG-PRIMITIVE-ATTRIBUTE>
                    <SDG-PRIMITIVE-ATTRIBUTE>
                      <SHORT-NAME>traceResponseSend</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">Enable / Disable tracing of transmitted ara::com method responses. Default: disabled.</L-2>
                      </DESC>
                      <CATEGORY>BOOLEAN</CATEGORY>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                    </SDG-PRIMITIVE-ATTRIBUTE>
                    <SDG-PRIMITIVE-ATTRIBUTE>
                      <SHORT-NAME>traceResponseReceived</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">Enable / Disable tracing of received ara::com method responses. Default: disabled.</L-2>
                      </DESC>
                      <CATEGORY>BOOLEAN</CATEGORY>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                    </SDG-PRIMITIVE-ATTRIBUTE>
                  </ATTRIBUTES>
                </SDG-CLASS>
                <SDG-CLASS>
                  <SHORT-NAME>TraceEventConfig</SHORT-NAME>
                  <ATTRIBUTES>
                    <SDG-PRIMITIVE-ATTRIBUTE>
                      <SHORT-NAME>traceSend</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">Enable / Disable tracing of transmitted ara::com events using the Skeleton::Event::Send(SampleType) API. Default: disabled.</L-2>
                      </DESC>
                      <CATEGORY>BOOLEAN</CATEGORY>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                    </SDG-PRIMITIVE-ATTRIBUTE>
                    <SDG-PRIMITIVE-ATTRIBUTE>
                      <SHORT-NAME>traceSendAllocate</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">Enable / Disable tracing of transmitted ara::com events using the Skeleton::Event::Send(SampleAllocateePtr) API. Default: disabled.</L-2>
                      </DESC>
                      <CATEGORY>BOOLEAN</CATEGORY>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                    </SDG-PRIMITIVE-ATTRIBUTE>
                    <SDG-PRIMITIVE-ATTRIBUTE>
                      <SHORT-NAME>traceReadSample</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">Enable / Disable tracing of read ara::com event samples. Default: disabled.

With AUTOSAR version R18-03: Trace emplacement of single event sample into the SampleContainer in context of GetCachedSamples() API.
Since AUTOSAR version R19-03: Trace execution of the user provided callable f given to the GetNewSamples() API.</L-2>
                      </DESC>
                      <CATEGORY>BOOLEAN</CATEGORY>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                    </SDG-PRIMITIVE-ATTRIBUTE>
                  </ATTRIBUTES>
                </SDG-CLASS>
                <SDG-CLASS>
                  <SHORT-NAME>TraceFieldNotifierConfig</SHORT-NAME>
                  <ATTRIBUTES>
                    <SDG-PRIMITIVE-ATTRIBUTE>
                      <SHORT-NAME>traceSend</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">Enable / Disable tracing of transmitted ara::com field notifier events using the Skeleton::Field::Update(FieldType) API. Default: disabled.</L-2>
                      </DESC>
                      <CATEGORY>BOOLEAN</CATEGORY>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                    </SDG-PRIMITIVE-ATTRIBUTE>
                    <SDG-PRIMITIVE-ATTRIBUTE>
                      <SHORT-NAME>traceReadSample</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">Enable / Disable tracing of read ara::com field notifier samples. Default: disabled.

With AUTOSAR version R18-03: Trace emplacement of single event sample into the SampleContainer in context of GetCachedSamples() API.
Since AUTOSAR version R19-03: Trace execution of the user provided callable f given to the GetNewSamples() API.</L-2>
                      </DESC>
                      <CATEGORY>BOOLEAN</CATEGORY>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                    </SDG-PRIMITIVE-ATTRIBUTE>
                  </ATTRIBUTES>
                </SDG-CLASS>
              </SDG-CLASSES>
            </SDG-DEF>
          </ELEMENTS>
        </AR-PACKAGE>
      </AR-PACKAGES>
    </AR-PACKAGE>
  </AR-PACKAGES>
</AUTOSAR>
