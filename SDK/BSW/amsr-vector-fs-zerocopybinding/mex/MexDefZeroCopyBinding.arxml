<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://autosar.org/schema/r4.0 AUTOSAR_00049.xsd">
  <AR-PACKAGES>
    <AR-PACKAGE>
      <SHORT-NAME>MICROSAR</SHORT-NAME>
      <AR-PACKAGES>
        <AR-PACKAGE>
          <SHORT-NAME>ADAPTIVE</SHORT-NAME>
          <ELEMENTS>
            <SDG-DEF>
              <SHORT-NAME>UserDefinedZeroCopyBindingExtensionModel</SHORT-NAME>
              <ADMIN-DATA>
                <SDGS>
                  <SDG GID="DvMex-version">
                    <SD>1.1.0</SD>
                  </SDG>
                </SDGS>
              </ADMIN-DATA>
              <SDG-CLASSES>
                <SDG-CLASS>
                  <SHORT-NAME>ProvidedZeroCopyBindingServiceInstanceExtension</SHORT-NAME>
                  <EXTENDS-META-CLASS>ProvidedUserDefinedServiceInstance</EXTENDS-META-CLASS>
                  <ATTRIBUTES>
                    <SDG-AGGREGATION-WITH-VARIATION>
                      <SHORT-NAME>userDefinedZeroCopyBinding</SHORT-NAME>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <VARIATION>false</VARIATION>
                      <SUB-SDG-REF DEST="SDG-CLASS">/MICROSAR/ADAPTIVE/UserDefinedZeroCopyBindingExtensionModel/ProvidedZeroCopyBindingServiceInstance</SUB-SDG-REF>
                    </SDG-AGGREGATION-WITH-VARIATION>
                  </ATTRIBUTES>
                </SDG-CLASS>
                <SDG-CLASS>
                  <SHORT-NAME>ProvidedZeroCopyBindingServiceInstance</SHORT-NAME>
                  <DESC>
                    <L-2 L="FOR-ALL">Stores the zero copy binding data of the ProvidedUserDefinedServiceInstance.</L-2>
                  </DESC>
                  <ATTRIBUTES>
                    <SDG-PRIMITIVE-ATTRIBUTE>
                      <SHORT-NAME>serviceInstanceId</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">The service instance ID (32 bit unsigned integer).</L-2>
                      </DESC>
                      <CATEGORY>INTEGER</CATEGORY>
                      <LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <MAX>0xFFFFFFFE</MAX>
                      <MIN>0x0</MIN>
                    </SDG-PRIMITIVE-ATTRIBUTE>
                    <SDG-AGGREGATION-WITH-VARIATION>
                      <SHORT-NAME>connectionEndpoint</SHORT-NAME>
                      <DESC>
                        <L-2 L="EN">The connection endpoint of this service instance.</L-2>
                      </DESC>
                      <LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <VARIATION>false</VARIATION>
                      <SUB-SDG-REF DEST="SDG-CLASS">/MICROSAR/ADAPTIVE/UserDefinedZeroCopyBindingExtensionModel/ProvidedServiceInstanceConnectionEndpoint</SUB-SDG-REF>
                    </SDG-AGGREGATION-WITH-VARIATION>
                    <SDG-AGGREGATION-WITH-VARIATION>
                      <SHORT-NAME>eventInstance</SHORT-NAME>
                      <DESC>
                        <L-2 L="EN">Stores the configuration of all events of the ProvidedZeroCopyBindingServiceInstance.</L-2>
                      </DESC>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY-INFINITE>true</UPPER-MULTIPLICITY-INFINITE>
                      <VARIATION>false</VARIATION>
                      <SUB-SDG-REF DEST="SDG-CLASS">/MICROSAR/ADAPTIVE/UserDefinedZeroCopyBindingExtensionModel/ProvidedEventInstance</SUB-SDG-REF>
                    </SDG-AGGREGATION-WITH-VARIATION>
                  </ATTRIBUTES>
                </SDG-CLASS>
                <SDG-CLASS>
                  <SHORT-NAME>RequiredZeroCopyBindingServiceInstanceExtension</SHORT-NAME>
                  <EXTENDS-META-CLASS>RequiredUserDefinedServiceInstance</EXTENDS-META-CLASS>
                  <ATTRIBUTES>
                    <SDG-AGGREGATION-WITH-VARIATION>
                      <SHORT-NAME>userDefinedZeroCopyBinding</SHORT-NAME>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <VARIATION>false</VARIATION>
                      <SUB-SDG-REF DEST="SDG-CLASS">/MICROSAR/ADAPTIVE/UserDefinedZeroCopyBindingExtensionModel/RequiredZeroCopyBindingServiceInstance</SUB-SDG-REF>
                    </SDG-AGGREGATION-WITH-VARIATION>
                  </ATTRIBUTES>
                </SDG-CLASS>
                <SDG-CLASS>
                  <SHORT-NAME>RequiredZeroCopyBindingServiceInstance</SHORT-NAME>
                  <DESC>
                    <L-2 L="EN">Stores the zero copy binding data of the RequiredUserDefinedServiceInstance.</L-2>
                  </DESC>
                  <ATTRIBUTES>
                    <SDG-PRIMITIVE-ATTRIBUTE>
                      <SHORT-NAME>serviceInstanceId</SHORT-NAME>
                      <DESC>
                        <L-2 L="EN">The service instance ID (32 bit unsigned integer as string or 'ALL').</L-2>
                      </DESC>
                      <CATEGORY>STRING</CATEGORY>
                      <LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <PATTERN>0|[1-9][0-9]*|ALL</PATTERN>
                    </SDG-PRIMITIVE-ATTRIBUTE>
                    <SDG-PRIMITIVE-ATTRIBUTE>
                      <SHORT-NAME>expectedMinimumServerIntegrityLevel</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">The expected minimum integrity level of the server (QM, ASIL_A, ASIL_B, ASIL_C or ASIL_D). Default: QM.</L-2>
                      </DESC>
                      <CATEGORY>ENUMERATION</CATEGORY>
                      <ADMIN-DATA>
                        <SDGS>
                          <SDG GID="literals">
                            <SD>QM</SD>
                            <SD>ASIL_A</SD>
                            <SD>ASIL_B</SD>
                            <SD>ASIL_C</SD>
                            <SD>ASIL_D</SD>
                          </SDG>
                        </SDGS>
                      </ADMIN-DATA>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                    </SDG-PRIMITIVE-ATTRIBUTE>
                    <SDG-AGGREGATION-WITH-VARIATION>
                      <SHORT-NAME>eventInstance</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">Stores the configuration of all events of the RequiredZeroCopyBindingServiceInstance.</L-2>
                      </DESC>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY-INFINITE>true</UPPER-MULTIPLICITY-INFINITE>
                      <VARIATION>false</VARIATION>
                      <SUB-SDG-REF DEST="SDG-CLASS">/MICROSAR/ADAPTIVE/UserDefinedZeroCopyBindingExtensionModel/RequiredEventInstance</SUB-SDG-REF>
                    </SDG-AGGREGATION-WITH-VARIATION>
                  </ATTRIBUTES>
                </SDG-CLASS>
                <SDG-CLASS>
                  <SHORT-NAME>ProvidedEventInstance</SHORT-NAME>
                  <DESC>
                    <L-2 L="EN">Stores the configuration of a single event instance.</L-2>
                  </DESC>
                  <ATTRIBUTES>
                    <SDG-REFERENCE>
                      <SHORT-NAME>event</SHORT-NAME>
                      <DESC>
                        <L-2 L="EN">The event this configuration refers to.</L-2>
                      </DESC>
                      <LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <DEST-SDG-REF DEST="SDG-CLASS">/MICROSAR/ADAPTIVE/UserDefinedZeroCopyBindingExtensionModel/ZeroCopyBindingEventDeployment</DEST-SDG-REF>
                    </SDG-REFERENCE>
                    <SDG-PRIMITIVE-ATTRIBUTE>
                      <SHORT-NAME>initialization</SHORT-NAME>
                      <DESC>
                        <L-2 L="EN">The type of initialization for this event when providing a pointer to the shared memory area to the sender.</L-2>
                      </DESC>
                      <CATEGORY>ENUMERATION</CATEGORY>
                      <ADMIN-DATA>
                        <SDGS>
                          <SDG GID="literals">
                            <SD>NoInitialization</SD>
                            <SD>ZeroInitialization</SD>
                            <SD>ConstructorInitialization</SD>
                          </SDG>
                        </SDGS>
                      </ADMIN-DATA>
                      <LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                    </SDG-PRIMITIVE-ATTRIBUTE>
                    <SDG-AGGREGATION-WITH-VARIATION>
                      <SHORT-NAME>qmLimits</SHORT-NAME>
                      <DESC>
                        <L-2 L="EN">The resource limits of this event valid for QM receivers.</L-2>
                      </DESC>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <VARIATION>false</VARIATION>
                      <SUB-SDG-REF DEST="SDG-CLASS">/MICROSAR/ADAPTIVE/UserDefinedZeroCopyBindingExtensionModel/ProvidedEventInstanceResourceLimit</SUB-SDG-REF>
                    </SDG-AGGREGATION-WITH-VARIATION>
                    <SDG-AGGREGATION-WITH-VARIATION>
                      <SHORT-NAME>asilALimits</SHORT-NAME>
                      <DESC>
                        <L-2 L="EN">The resource limits of this event valid for ASIL A receivers.</L-2>
                      </DESC>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <VARIATION>false</VARIATION>
                      <SUB-SDG-REF DEST="SDG-CLASS">/MICROSAR/ADAPTIVE/UserDefinedZeroCopyBindingExtensionModel/ProvidedEventInstanceResourceLimit</SUB-SDG-REF>
                    </SDG-AGGREGATION-WITH-VARIATION>
                    <SDG-AGGREGATION-WITH-VARIATION>
                      <SHORT-NAME>asilBLimits</SHORT-NAME>
                      <DESC>
                        <L-2 L="EN">The resource limits of this event valid for ASIL B receivers.</L-2>
                      </DESC>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <VARIATION>false</VARIATION>
                      <SUB-SDG-REF DEST="SDG-CLASS">/MICROSAR/ADAPTIVE/UserDefinedZeroCopyBindingExtensionModel/ProvidedEventInstanceResourceLimit</SUB-SDG-REF>
                    </SDG-AGGREGATION-WITH-VARIATION>
                    <SDG-AGGREGATION-WITH-VARIATION>
                      <SHORT-NAME>asilCLimits</SHORT-NAME>
                      <DESC>
                        <L-2 L="EN">The resource limits of this event valid for ASIL C receivers.</L-2>
                      </DESC>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <VARIATION>false</VARIATION>
                      <SUB-SDG-REF DEST="SDG-CLASS">/MICROSAR/ADAPTIVE/UserDefinedZeroCopyBindingExtensionModel/ProvidedEventInstanceResourceLimit</SUB-SDG-REF>
                    </SDG-AGGREGATION-WITH-VARIATION>
                    <SDG-AGGREGATION-WITH-VARIATION>
                      <SHORT-NAME>asilDLimits</SHORT-NAME>
                      <DESC>
                        <L-2 L="EN">The resource limits of this event valid for ASIL D receivers.</L-2>
                      </DESC>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <VARIATION>false</VARIATION>
                      <SUB-SDG-REF DEST="SDG-CLASS">/MICROSAR/ADAPTIVE/UserDefinedZeroCopyBindingExtensionModel/ProvidedEventInstanceResourceLimit</SUB-SDG-REF>
                    </SDG-AGGREGATION-WITH-VARIATION>
                    <SDG-AGGREGATION-WITH-VARIATION>
                      <SHORT-NAME>allocationMemoryTechnology</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">The memory technology used when allocating memory for the event. Default: SharedMemory.</L-2>
                      </DESC>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <VARIATION>false</VARIATION>
                      <SUB-SDG-REF DEST="SDG-CLASS">/MICROSAR/ADAPTIVE/UserDefinedZeroCopyBindingExtensionModel/EventInstanceMemoryTechnology</SUB-SDG-REF>
                    </SDG-AGGREGATION-WITH-VARIATION>
                  </ATTRIBUTES>
                </SDG-CLASS>
                <SDG-CLASS>
                  <SHORT-NAME>ProvidedEventInstanceResourceLimit</SHORT-NAME>
                  <DESC>
                    <L-2 L="EN">Stores the resource limits of a single event.</L-2>
                  </DESC>
                  <ATTRIBUTES>
                    <SDG-PRIMITIVE-ATTRIBUTE>
                      <SHORT-NAME>slotsLimit</SHORT-NAME>
                      <DESC>
                        <L-2 L="EN">The slot limit (32 bit unsigned integer).</L-2>
                      </DESC>
                      <CATEGORY>INTEGER</CATEGORY>
                      <LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <MAX>0xFFFFFFFF</MAX>
                      <MIN>0x1</MIN>
                    </SDG-PRIMITIVE-ATTRIBUTE>
                    <SDG-PRIMITIVE-ATTRIBUTE>
                      <SHORT-NAME>connectionsLimit</SHORT-NAME>
                      <DESC>
                        <L-2 L="EN">The connection limit (32 bit unsigned integer).</L-2>
                      </DESC>
                      <CATEGORY>INTEGER</CATEGORY>
                      <LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <MAX>0xFFFFFFFF</MAX>
                      <MIN>0x1</MIN>
                    </SDG-PRIMITIVE-ATTRIBUTE>
                  </ATTRIBUTES>
                </SDG-CLASS>
                <SDG-CLASS>
                  <SHORT-NAME>ProvidedServiceInstanceConnectionEndpoint</SHORT-NAME>
                  <DESC>
                    <L-2 L="EN">Stores the address information of a connection endpoint.</L-2>
                  </DESC>
                  <ATTRIBUTES>
                    <SDG-PRIMITIVE-ATTRIBUTE>
                      <SHORT-NAME>domain</SHORT-NAME>
                      <DESC>
                        <L-2 L="EN">The domain (32 bit unsigned integer).</L-2>
                      </DESC>
                      <CATEGORY>INTEGER</CATEGORY>
                      <LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <MAX>0xFFFFFFFF</MAX>
                      <MIN>0x0</MIN>
                    </SDG-PRIMITIVE-ATTRIBUTE>
                    <SDG-PRIMITIVE-ATTRIBUTE>
                      <SHORT-NAME>port</SHORT-NAME>
                      <DESC>
                        <L-2 L="EN">The port (32 bit unsigned integer).</L-2>
                      </DESC>
                      <CATEGORY>INTEGER</CATEGORY>
                      <LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <MAX>0xFFFFFFFF</MAX>
                      <MIN>0x0</MIN>
                    </SDG-PRIMITIVE-ATTRIBUTE>
                  </ATTRIBUTES>
                </SDG-CLASS>
                <SDG-CLASS>
                  <SHORT-NAME>ZeroCopyBindingServiceInterfaceDeploymentExtension</SHORT-NAME>
                  <EXTENDS-META-CLASS>UserDefinedServiceInterfaceDeployment</EXTENDS-META-CLASS>
                  <ATTRIBUTES>
                    <SDG-AGGREGATION-WITH-VARIATION>
                      <SHORT-NAME>userDefinedZeroCopyBinding</SHORT-NAME>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <VARIATION>false</VARIATION>
                      <SUB-SDG-REF DEST="SDG-CLASS">/MICROSAR/ADAPTIVE/UserDefinedZeroCopyBindingExtensionModel/ZeroCopyBindingServiceInterfaceDeployment</SUB-SDG-REF>
                    </SDG-AGGREGATION-WITH-VARIATION>
                  </ATTRIBUTES>
                </SDG-CLASS>
                <SDG-CLASS>
                  <SHORT-NAME>ZeroCopyBindingServiceInterfaceDeployment</SHORT-NAME>
                  <DESC>
                    <L-2 L="FOR-ALL">Stores the zero copy binding data of the UserDefinedServiceInterfaceDeployment.</L-2>
                  </DESC>
                  <ATTRIBUTES>
                    <SDG-PRIMITIVE-ATTRIBUTE>
                      <SHORT-NAME>serviceInterfaceId</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">The service interface ID (32 bit unsigned integer).</L-2>
                      </DESC>
                      <CATEGORY>INTEGER</CATEGORY>
                      <LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <MAX>0xFFFFFFFF</MAX>
                      <MIN>0x0</MIN>
                    </SDG-PRIMITIVE-ATTRIBUTE>
                  </ATTRIBUTES>
                </SDG-CLASS>
                <SDG-CLASS>
                  <SHORT-NAME>ZeroCopyBindingEventDeploymentExtension</SHORT-NAME>
                  <EXTENDS-META-CLASS>UserDefinedEventDeployment</EXTENDS-META-CLASS>
                  <ATTRIBUTES>
                    <SDG-AGGREGATION-WITH-VARIATION>
                      <SHORT-NAME>userDefinedZeroCopyBinding</SHORT-NAME>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <VARIATION>false</VARIATION>
                      <SUB-SDG-REF DEST="SDG-CLASS">/MICROSAR/ADAPTIVE/UserDefinedZeroCopyBindingExtensionModel/ZeroCopyBindingEventDeployment</SUB-SDG-REF>
                    </SDG-AGGREGATION-WITH-VARIATION>
                  </ATTRIBUTES>
                </SDG-CLASS>
                <SDG-CLASS>
                  <SHORT-NAME>ZeroCopyBindingEventDeployment</SHORT-NAME>
                  <DESC>
                    <L-2 L="FOR-ALL">Stores the zero copy binding data of the UserDefinedEventDeployment.</L-2>
                  </DESC>
                  <CAPTION>true</CAPTION>
                  <ATTRIBUTES>
                    <SDG-PRIMITIVE-ATTRIBUTE>
                      <SHORT-NAME>eventId</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">The event ID (32 bit unsigned integer).</L-2>
                      </DESC>
                      <CATEGORY>INTEGER</CATEGORY>
                      <LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <MAX>0xFFFFFFFF</MAX>
                      <MIN>0x0</MIN>
                    </SDG-PRIMITIVE-ATTRIBUTE>
                  </ATTRIBUTES>
                </SDG-CLASS>
                <SDG-CLASS>
                  <SHORT-NAME>EventInstanceMemoryTechnology</SHORT-NAME>
                  <DESC>
                    <L-2 L="FOR-ALL">The memory technology used for a event.</L-2>
                  </DESC>
                  <ATTRIBUTES>
                    <SDG-PRIMITIVE-ATTRIBUTE>
                      <SHORT-NAME>memoryTechnology</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">The memory technology.</L-2>
                      </DESC>
                      <CATEGORY>ENUMERATION</CATEGORY>
                      <ADMIN-DATA>
                        <SDGS>
                          <SDG GID="literals">
                            <SD>SharedMemory</SD>
                            <SD>CustomMemory</SD>
                          </SDG>
                        </SDGS>
                      </ADMIN-DATA>
                      <LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                    </SDG-PRIMITIVE-ATTRIBUTE>
                  </ATTRIBUTES>
                </SDG-CLASS>
                <SDG-CLASS>
                  <SHORT-NAME>RequiredEventInstance</SHORT-NAME>
                  <DESC>
                    <L-2 L="FOR-ALL">Stores the configuration of a single event instance.</L-2>
                  </DESC>
                  <ATTRIBUTES>
                    <SDG-REFERENCE>
                      <SHORT-NAME>event</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">The event this configuration refers to.</L-2>
                      </DESC>
                      <LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <DEST-SDG-REF DEST="SDG-CLASS">/MICROSAR/ADAPTIVE/UserDefinedZeroCopyBindingExtensionModel/ZeroCopyBindingEventDeployment</DEST-SDG-REF>
                    </SDG-REFERENCE>
                    <SDG-AGGREGATION-WITH-VARIATION>
                      <SHORT-NAME>allocationMemoryTechnology</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">The memory technology used when allocating memory for the event. Default: SharedMemory.</L-2>
                      </DESC>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <VARIATION>false</VARIATION>
                      <SUB-SDG-REF DEST="SDG-CLASS">/MICROSAR/ADAPTIVE/UserDefinedZeroCopyBindingExtensionModel/EventInstanceMemoryTechnology</SUB-SDG-REF>
                    </SDG-AGGREGATION-WITH-VARIATION>
                  </ATTRIBUTES>
                </SDG-CLASS>
              </SDG-CLASSES>
            </SDG-DEF>
          </ELEMENTS>
        </AR-PACKAGE>
      </AR-PACKAGES>
    </AR-PACKAGE>
  </AR-PACKAGES>
</AUTOSAR>
