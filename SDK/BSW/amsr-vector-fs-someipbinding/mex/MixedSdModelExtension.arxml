<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://autosar.org/schema/r4.0 AUTOSAR_00049.xsd">
  <AR-PACKAGES>
    <AR-PACKAGE>
      <SHORT-NAME>MICROSAR</SHORT-NAME>
      <AR-PACKAGES>
        <AR-PACKAGE UUID="0bdb538a-3fc2-4643-8897-171a15a4a207">
          <SHORT-NAME>ADAPTIVE</SHORT-NAME>
          <ELEMENTS>
            <SDG-DEF>
              <SHORT-NAME>Mex4MixedSd</SHORT-NAME>
              <SDG-CLASSES>
                <SDG-CLASS>
                  <SHORT-NAME>MixedSdSomeipServiceInstanceToMachineMapping</SHORT-NAME>
                  <EXTENDS-META-CLASS>SomeipServiceInstanceToMachineMapping</EXTENDS-META-CLASS>
                  <ATTRIBUTES>
                    <SDG-AGGREGATION-WITH-VARIATION>
                      <SHORT-NAME>staticServiceDiscovery</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">This optional attribute is used for disabling Service Discovery as defined
                         by the AUTOSAR Service Discovery protocol. It allows manual and static configuration of 
                         the addresses where service providers and event consumers can be found. </L-2>
                      </DESC>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <SUB-SDG-REF DEST="SDG-CLASS">/MICROSAR/ADAPTIVE/Mex4MixedSd/StaticServiceDiscovery</SUB-SDG-REF>
                    </SDG-AGGREGATION-WITH-VARIATION>
                  </ATTRIBUTES>
                </SDG-CLASS>
                <SDG-CLASS>
                  <SHORT-NAME>StaticServiceDiscovery</SHORT-NAME>
                  <ATTRIBUTES>
                    <SDG-AGGREGATION-WITH-VARIATION>
                      <SHORT-NAME>serviceProvider</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">This optional attribute represents the IP address where a provided 
                        service can be found.</L-2>
                      </DESC>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <SUB-SDG-REF DEST="SDG-CLASS">/MICROSAR/ADAPTIVE/Mex4MixedSd/ServiceProvider</SUB-SDG-REF>
                    </SDG-AGGREGATION-WITH-VARIATION>
                    <SDG-AGGREGATION-WITH-VARIATION>
                      <SHORT-NAME>eventSubscribers</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">This optional attribute represents a container of static subscribers to a 
                        provided eventgroup.</L-2>
                      </DESC>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <SUB-SDG-REF DEST="SDG-CLASS">/MICROSAR/ADAPTIVE/Mex4MixedSd/EventSubscribers</SUB-SDG-REF>
                    </SDG-AGGREGATION-WITH-VARIATION>
                  </ATTRIBUTES>
                </SDG-CLASS>
                <SDG-CLASS>
                  <SHORT-NAME>EventSubscribers</SHORT-NAME>
                  <ATTRIBUTES>
                    <SDG-AGGREGATION-WITH-VARIATION>
                      <SHORT-NAME>subscriber</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">This optional attribute represents a list of eventgroup subscribers.</L-2>
                      </DESC>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY-INFINITE>true</UPPER-MULTIPLICITY-INFINITE>
                      <SUB-SDG-REF DEST="SDG-CLASS">/MICROSAR/ADAPTIVE/Mex4MixedSd/Subscriber</SUB-SDG-REF>
                    </SDG-AGGREGATION-WITH-VARIATION>
                  </ATTRIBUTES>
                </SDG-CLASS>
                <SDG-CLASS>
                  <SHORT-NAME>Subscriber</SHORT-NAME>
                  <ATTRIBUTES>
                    <SDG-PRIMITIVE-ATTRIBUTE>
                      <SHORT-NAME>ipAddress</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">This attribute represents the IP Address of an event consumer.</L-2>
                      </DESC>
                      <CATEGORY>STRING</CATEGORY>
                      <LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                    </SDG-PRIMITIVE-ATTRIBUTE>
                    <SDG-PRIMITIVE-ATTRIBUTE>
                      <SHORT-NAME>udpPort</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">This attribute represents the port of an event consumer.</L-2>
                      </DESC>
                      <CATEGORY>INTEGER</CATEGORY>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                    </SDG-PRIMITIVE-ATTRIBUTE>
                    <SDG-AGGREGATION-WITH-VARIATION>
                      <SHORT-NAME>eventgroups</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">This optional attribute represents a list of eventgroups to which a subscriber
                         is statically subscribed to.</L-2>
                      </DESC>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY-INFINITE>true</UPPER-MULTIPLICITY-INFINITE>
                      <SUB-SDG-REF DEST="SDG-CLASS">/MICROSAR/ADAPTIVE/Mex4MixedSd/EventGroup</SUB-SDG-REF>
                    </SDG-AGGREGATION-WITH-VARIATION>
                  </ATTRIBUTES>
                </SDG-CLASS>
                <SDG-CLASS>
                  <SHORT-NAME>EventGroup</SHORT-NAME>
                  <ATTRIBUTES>
                    <SDG-PRIMITIVE-ATTRIBUTE>
                      <SHORT-NAME>eventgroup</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">This attribute represents the ID of an eventgroup.</L-2>
                      </DESC>
                      <CATEGORY>INTEGER</CATEGORY>
                      <LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                    </SDG-PRIMITIVE-ATTRIBUTE>
                  </ATTRIBUTES>
                </SDG-CLASS>
                <SDG-CLASS>
                  <SHORT-NAME>ServiceProvider</SHORT-NAME>
                  <ATTRIBUTES>
                    <SDG-PRIMITIVE-ATTRIBUTE>
                      <SHORT-NAME>ipAddress</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">This attribute provides the ipAddress of the ServiceProvider.</L-2>
                      </DESC>
                      <CATEGORY>STRING</CATEGORY>
                      <LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                    </SDG-PRIMITIVE-ATTRIBUTE>
                    <SDG-PRIMITIVE-ATTRIBUTE>
                      <SHORT-NAME>tcpPort</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">This optional attribute represents the tcpPort of the ServiceProvider.</L-2>
                      </DESC>
                      <CATEGORY>INTEGER</CATEGORY>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                    </SDG-PRIMITIVE-ATTRIBUTE>
                    <SDG-PRIMITIVE-ATTRIBUTE>
                      <SHORT-NAME>udpPort</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">This optional attribute represents the udpPort of the ServiceProvider.</L-2>
                      </DESC>
                      <CATEGORY>INTEGER</CATEGORY>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                    </SDG-PRIMITIVE-ATTRIBUTE>
                    <SDG-PRIMITIVE-ATTRIBUTE>
                      <SHORT-NAME>multicastAddress</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">This optional attribute represents the multicastAddress of the 
                        ServiceProvider.</L-2>
                      </DESC>
                      <CATEGORY>STRING</CATEGORY>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                    </SDG-PRIMITIVE-ATTRIBUTE>
                    <SDG-PRIMITIVE-ATTRIBUTE>
                      <SHORT-NAME>multicastPort</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">This optional attribute represents the multicastPort of the 
                        ServiceProvider.</L-2>
                      </DESC>
                      <CATEGORY>INTEGER</CATEGORY>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                    </SDG-PRIMITIVE-ATTRIBUTE>
                  </ATTRIBUTES>
                </SDG-CLASS>
              </SDG-CLASSES>
            </SDG-DEF>
          </ELEMENTS>
        </AR-PACKAGE>
      </AR-PACKAGES>
    </AR-PACKAGE>
  </AR-PACKAGES>
</AUTOSAR>
