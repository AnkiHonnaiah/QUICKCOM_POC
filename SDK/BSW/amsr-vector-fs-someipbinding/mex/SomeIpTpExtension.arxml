<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://autosar.org/schema/r4.0 AUTOSAR_00049.xsd">
  <AR-PACKAGES>
    <AR-PACKAGE>
      <SHORT-NAME>MICROSAR</SHORT-NAME>
      <AR-PACKAGES>
        <AR-PACKAGE>
          <SHORT-NAME>ADAPTIVE</SHORT-NAME>
          <ELEMENTS>
            <SDG-DEF>
              <SHORT-NAME>SomeIpTpExtension</SHORT-NAME>
              <SDG-CLASSES>
                <SDG-CLASS>
                  <SHORT-NAME>SomeipMethodDeploymentTpExtension</SHORT-NAME>
                  <EXTENDS-META-CLASS>SomeipMethodDeployment</EXTENDS-META-CLASS>
                  <ATTRIBUTES>
                    <SDG-PRIMITIVE-ATTRIBUTE>
                      <SHORT-NAME>preallocatedRxBufferCount</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">Specifies the number of rx buffers that shall be pre-allocated for receiving SOME/IP-TP messages. If not configured, dynamic allocation will be applied.</L-2>
                      </DESC>
                      <CATEGORY>INTEGER</CATEGORY>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <MAX>4294967295</MAX>
                      <MIN>1</MIN>
                    </SDG-PRIMITIVE-ATTRIBUTE>
                  </ATTRIBUTES>
                </SDG-CLASS>
                <SDG-CLASS>
                  <SHORT-NAME>SomeipEventDeploymentTpExtension</SHORT-NAME>
                  <EXTENDS-META-CLASS>SomeipEventDeployment</EXTENDS-META-CLASS>
                  <ATTRIBUTES>
                    <SDG-PRIMITIVE-ATTRIBUTE>
                      <SHORT-NAME>preallocatedRxBufferCount</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">Specifies the number of rx buffers that shall be pre-allocated for receiving SOME/IP-TP messages. If not configured, dynamic allocation will be applied.</L-2>
                      </DESC>
                      <CATEGORY>INTEGER</CATEGORY>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                      <MAX>4294967295</MAX>
                      <MIN>1</MIN>
                    </SDG-PRIMITIVE-ATTRIBUTE>
                  </ATTRIBUTES>
                </SDG-CLASS>
              </SDG-CLASSES>
            </SDG-DEF>
          </ELEMENTS>
        </AR-PACKAGE>
      </AR-PACKAGES>
    </AR-PACKAGE>
  </AR-PACKAGES>
</AUTOSAR>