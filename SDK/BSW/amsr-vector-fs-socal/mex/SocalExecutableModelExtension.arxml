<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://autosar.org/schema/r4.0 AUTOSAR_00049.xsd">
  <AR-PACKAGES>
    <AR-PACKAGE>
      <SHORT-NAME>MICROSAR</SHORT-NAME>
      <AR-PACKAGES>
        <AR-PACKAGE>
          <SHORT-NAME>ADAPTIVE</SHORT-NAME>
          <ELEMENTS>
            <SDG-DEF>
              <SHORT-NAME>SocalModelExtension</SHORT-NAME>
              <SDG-CLASSES>
                <SDG-CLASS>
                  <SHORT-NAME>SocalExecutableModelExtension</SHORT-NAME>
                  <EXTENDS-META-CLASS>Executable</EXTENDS-META-CLASS>
                  <ATTRIBUTES>
                    <SDG-PRIMITIVE-ATTRIBUTE>
                      <SHORT-NAME>araComRuntimeProcessingMode</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">The processing mode defines if reactor events are handled in a thread driven manner or polling based.
                                         Further, the selected mode influences the set of available APIs.
                                         If not set, it defaults to ThreadDriven mode.
                        </L-2>
                      </DESC>
                      <CATEGORY>ENUMERATION</CATEGORY>
                      <ADMIN-DATA>
                        <SDGS>
                          <SDG GID="literals">
                            <SD>ThreadDriven</SD>
                            <SD>Polling</SD>
                          </SDG>
                        </SDGS>
                      </ADMIN-DATA>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                    </SDG-PRIMITIVE-ATTRIBUTE>
                    <SDG-FOREIGN-REFERENCE>
                      <SHORT-NAME>genericProxyServiceInterfaceRef</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">List of service interface references that are used with generic proxy.</L-2>
                      </DESC>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY-INFINITE>true</UPPER-MULTIPLICITY-INFINITE>
                      <DEST-META-CLASS>ServiceInterface</DEST-META-CLASS>
                    </SDG-FOREIGN-REFERENCE>
                  </ATTRIBUTES>
                </SDG-CLASS>
              </SDG-CLASSES>
            </SDG-DEF>
          </ELEMENTS>
        </AR-PACKAGE>
      </AR-PACKAGES>
    </AR-PACKAGE>
  </AR-PACKAGES>
</AUTOSAR>
