<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://autosar.org/schema/r4.0 AUTOSAR_00049.xsd">
  <AR-PACKAGES>
    <AR-PACKAGE>
      <SHORT-NAME>MICROSAR</SHORT-NAME>
      <AR-PACKAGES>
        <AR-PACKAGE>
          <SHORT-NAME>ADAPTIVE</SHORT-NAME>
          <ELEMENTS>
            <SDG-DEF>
              <SHORT-NAME>BswSteadyModeExtensionModel</SHORT-NAME>
              <ADMIN-DATA>
                <SDGS>
                  <SDG GID="DvMex-version">
                    <SD>1.0.0</SD>
                  </SDG>
                </SDGS>
              </ADMIN-DATA>
              <SDG-CLASSES>
                <SDG-CLASS>
                  <SHORT-NAME>BswSteadyMode</SHORT-NAME>
                  <DESC>
                    <L-2 L="EN">Usage of this extension model shall denote usage of Steady Mode in a MICROSAR Adaptive application.</L-2>
                  </DESC>
                  <EXTENDS-META-CLASS>Process</EXTENDS-META-CLASS>
                  <ATTRIBUTES>
                    <SDG-PRIMITIVE-ATTRIBUTE>
                      <SHORT-NAME>steadyMode</SHORT-NAME>
                      <DESC>
                        <L-2 L="FOR-ALL">This attribute indicates Steady Mode shall be used for all steady-enabled API.</L-2>
                      </DESC>
                      <CATEGORY>BOOLEAN</CATEGORY>
                      <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                    </SDG-PRIMITIVE-ATTRIBUTE>
                  </ATTRIBUTES>
                </SDG-CLASS>
              </SDG-CLASSES>
            </SDG-DEF>
          </ELEMENTS>
        </AR-PACKAGE>
      </AR-PACKAGES>
    </AR-PACKAGE>
  </AR-PACKAGES>
</AUTOSAR>
